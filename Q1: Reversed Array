# Question 1:
# Write a program to reverse an array or string using loop
# input: [ 1, 2, 3 ]
# output: [ 3, 2, 1 ]

Answer:

function reverseArray(arr) {
  let reversedArray = [];
  for (let i = arr.length - 1; i >= 0; i--) {
    reversedArray[reversedArray.length] = arr[i];
  }
  return reversedArray;
}
